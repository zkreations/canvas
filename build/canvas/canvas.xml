<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE html>
<html b:css="false" b:js="false" b:defaultwidgetversion="2" b:layoutsVersion="3" b:responsive="true" expr:dir="data:blog.locale.languageDirection" expr:lang="data:blog.locale.language" prefix="og: http://ogp.me/ns#">
<head>
  <b:include name="canvas:head"/>
  <b:skin><![CDATA[
    /*!
    => Theme: YOUR_THEME_NAME
    => Version: 1.0.0
    => License: GNU General Public License v2 or later
    => Core: Canvas v2.0.0-beta1 (https://github.com/zkreations/canvas)
    */ 
    /*
     
    { Emporio }
    ====================
    <Variable name="posts.text.color" description="Color" type="string" value="#666"/>
    <Variable name="posts.text.font" description="Font" type="string" value="15px Roboto"/>
    <Variable name="posts.link.color" description="Link" type="string" value="#666"/>
    <Variable name="posts.background.color" description="Bg" type="string" value="transparent"/>
    <Variable name="posts.title.color" description="none1" type="string" value="0"/>
    <Variable name="body.link.color" description="none2" type="string" value="0"/>
    <Variable name="body.text.font" description="none3" type="string" value="0"/>
     
    { Configuration }
    ====================
    <Variable name="c.comments" description="Comments system" type="string" value="blogger"/>
    <Variable name="c.newFirst" description="New comments first" type="string" value="true"/>
    <Variable name="c.linkFilter" description="Enable link filter" type="string" value="true"/>
    <Variable name="c.replies" description="Enable replies" type="string" value="true"/>
     
    { Defaults }
    ====================
    <Variable name="d.image" description="Imagen" type="string"/>
    <Variable name="d.avatar" description="Avatar" type="string" value="https://lh3.googleusercontent.com/a/default-user"/>
     
    { Texts }
    ====================
    <Variable name="t.404" description="Error 404" type="string" value="Error 404"/>
    <Variable name="t.reply" description="Reply" type="string" value="Responder"/>
    <Variable name="t.nolinks" description="no Links" type="string" value="Los enlaces no estÃ¡n permitidos"/>
     
    { Customize }
    ====================
    <Variable name="keycolor" description="keycolor" type="string" default="#2196f3"/>
    <Group description="Backgrounds" selector="body">
      <Variable name="body.background" description="Background" type="background" color="$(body.background.color)" default="$(color) none repeat scroll top center"/>
      <Variable name="body.background.color" description="Body background color" type="color" default="#fff"/>
    </Group>
    */
    /*! root.css */
    ]]>
  </b:skin>
  <style>/*<![CDATA[*//*! main.css */
    /*]]>*/
  </style>
  <b:if cond="data:view.isLayoutMode">
    <b:template-skin><![CDATA[/*! admin.css */
      ]]>
    </b:template-skin>
  </b:if>
  <b:defaultmarkups>
    <b:defaultmarkup type="Common">
      <b:includable id="canvas:head">
        <meta expr:charset="data:blog.encoding"></meta>
        <meta content="width=device-width,initial-scale=1" name="viewport"></meta>
        <title>
          <b:eval expr="data:view.isError ? data:skin.vars.t_404 : data:view.title.escaped"/>
        </title>
        <meta expr:content="data:view.description.escaped" name="description"></meta>
        <link expr:href="data:view.url.canonical" rel="canonical"></link>
        <link expr:href="data:blog.blogspotFaviconUrl" rel="icon" type="image/x-icon"></link>
        <b:tag cond="data:appId" expr:content="data:appId" name="meta" property="fb:app_id"/>
        <meta expr:content="!data:view.isSingleItem ? &quot;article&quot; : &quot;website&quot;" property="og:type"></meta>
        <meta expr:content="data:blog.title" property="og:site_name"></meta>
        <b:include cond="!data:view.isError" name="openGraphMetaData"/>
        <b:if cond="data:view.isHomepage or data:view.isSingleItem">
          <meta content="index,follow,max-snippet:-1,max-image-preview:large,max-video-preview:-1" name="robots"></meta>
          <b:else/>
          <meta content="noindex,follow" name="robots"></meta>
        </b:if>
        <meta content="summary_large_image" name="twitter:card"></meta>
        <b:if cond="data:blog.analyticsAccountNumber">
          <script async="async" expr:src="params(&quot;https://www.googletagmanager.com/gtag/js&quot;,{id: data:blog.analyticsAccountNumber})"></script>
          <script>
            window.dataLayer = window.dataLayer || [];
            function gtag(){dataLayer.push(arguments);}
            gtag('js', new Date());
            gtag('config','<data:blog.analyticsAccountNumber/>');
          </script>
        </b:if>
        <b:tag defer="defer" name="script" cond="data:blog.adsenseClientId" src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></b:tag>
      </b:includable>
      <b:includable id="canvas:image">
        <b:with value="resizeImage((data:src.isYoutube ? data:src.youtubeMaxResDefaultUrl : data:src), (data:resize ?: 1280), data:ratio)" var="image">
          <b:tag expr:alt="data:alt ?: data:messages.image" name="img">
            <b:attr name="id" expr:value="data:id"/>
            <b:attr name="width" expr:value="data:width ?: data:image.width"/>
            <b:attr name="height" expr:value="data:height ?: data:image.height"/>
            <b:attr name="sizes" expr:value="data:sizes"/>
            <b:class cond="data:class" expr:name="data:class"/>
            <b:attr cond="data:loading" name="loading" value="lazy"/>
            <b:class cond="data:lazyload" expr:name="data:lazyload"/>
            <b:with value="data:lazyload ? &quot;data-&quot; : &quot;&quot;" var="data">
              <b:attr cond="data:data" name="src" value="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw=="></b:attr>
              <b:attr cond="data:srcset" expr:name="data:data + &quot;srcset&quot;" expr:value="sourceSet(data:image, (data:srcset ?: []), data:ratio)"/>
              <b:attr expr:name="data:data + &quot;src&quot;" expr:value="data:srcset ? resizeImage(data:image, data:srcset.first, data:ratio) : data:image"/>
            </b:with>
          </b:tag>
          <b:if cond="data:noscript">
            <noscript>
              <b:include name="canvas:image" data="{ class: (data:class ?: false), src: data:image, resize: (data:resize ?: (data:srcset.first ?: 300)), ratio: data:ratio }"/>
            </noscript>
          </b:if>
        </b:with>
      </b:includable>
      <b:includable id="canvas:bodyClass">
        <b:class cond="data:view.isHomepage" name="is-home"/>
        <b:class cond="data:view.isMultipleItems" name="is-feed"/>
        <b:class cond="data:view.isSingleItem" name="is-single"/>
        <b:class cond="data:view.isPost" name="is-post"/>
        <b:class cond="data:view.isPage" name="is-page"/>
        <b:class cond="data:view.isArchive" name="is-archive"/>
        <b:class cond="data:view.isLabelSearch" name="is-label"/>
        <b:class cond="data:view.isSearch" name="is-search"/>
        <b:class cond="data:view.isError" name="is-error"/>
        <b:class cond="data:view.isPreview" name="is-preview"/>
        <b:if cond="data:entryId">
          <b:class cond="data:view.isSingleItem" expr:name="+(&quot;entry-id-&quot;,(data:view.postId ?: data:view.pageId))"/>
        </b:if>
      </b:includable>
      <b:includable id="canvas:setAttr" var="array">
        <b:loop values="data:array" var="i">
          <b:attr expr:name="data:i[0]" expr:value="data:i[1]"/>
        </b:loop>
      </b:includable>
      <b:includable id="canvas:truncate">
        <b:eval expr="data:string snippet { length: (data:length ?: 50), links: false, linebreaks: false, ellipsis: (data:ellipsis ?: false) }"/>
      </b:includable>
    </b:defaultmarkup>
    <b:defaultmarkup type="BlogList,ContactForm,Feed,HTML,Image,LinkList,Stats,TextList,Translate,Wikipedia">
      <b:includable id="content"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Blog,PopularPosts,FeaturedPost">
      <b:includable id="blogThisShare"/>
      <b:includable id="bylineByName"/>
      <b:includable id="bylineRegion"/>
      <b:includable id="commentsLink"/>
      <b:includable id="commentsLinkIframe"/>
      <b:includable id="emailPostIcon"/>
      <b:includable id="facebookShare"/>
      <b:includable id="footerBylines"/>
      <b:includable id="headerByline"/>
      <b:includable id="googlePlusShare"/>
      <b:includable id="linkShare"/>
      <b:includable id="otherSharingButton"/>
      <b:includable id="platformShare"/>
      <b:includable id="postAuthor"/>
      <b:includable id="postCommentsLink"/>
      <b:includable id="postJumpLink"/>
      <b:includable id="postLabels"/>
      <b:includable id="postLocation"/>
      <b:includable id="postReactions"/>
      <b:includable id="postShareButtons"/>
      <b:includable id="postTimestamp"/>
      <b:includable id="sharingButton"/>
      <b:includable id="sharingButtonContent"/>
      <b:includable id="sharingButtons"/>
      <b:includable id="sharingButtonsMenu"/>
      <b:includable id="sharingPlatformIcon"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="PopularPosts,FeaturedPost">
      <b:includable id="snippetedPostByline"/>
      <b:includable id="snippetedPostContent"/>
      <b:includable id="snippetedPosts"/>
      <b:includable id="snippetedPostThumbnail"/>
      <b:includable id="snippetedPostTitle"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Blog">
      <b:includable id="aboutPostAuthor"/>
      <b:includable id="addComments"/>
      <b:includable id="commentAuthorAvatar"/>
      <b:includable id="commentDeleteIcon"/>
      <b:includable id="commentForm"/>
      <b:includable id="commentFormIframeSrc"/>
      <b:includable id="commentItem"/>
      <b:includable id="commentList"/>
      <b:includable id="commentPicker"/>
      <b:includable id="comments"/>
      <b:includable id="commentsTitle"/>
      <b:includable id="defaultAdUnit"/>
      <b:includable id="feedLinks"/>
      <b:includable id="feedLinksBody"/>
      <b:includable id="homePageLink"/>
      <b:includable id="iframeComments"/>
      <b:includable id="inlineAd"/>
      <b:includable id="manageComments"/>
      <b:includable id="nextPageLink"/>
      <b:includable id="post"/>
      <b:includable id="postBody"/>
      <b:includable id="postBodySnippet"/>
      <b:includable id="postCommentsAndAd"/>
      <b:includable id="postFooter"/>
      <b:includable id="postFooterAuthorProfile"/>
      <b:includable id="postHeader"/>
      <b:includable id="postMeta"/>
      <b:includable id="postMetadataJSONImage"/>
      <b:includable id="postMetadataJSONPublisher"/>
      <b:includable id="postPagination"/>
      <b:includable id="postTitle"/>
      <b:includable id="previousPageLink"/>
      <b:includable id="threadedCommentForm"/>
      <b:includable id="threadedCommentJs"/>
      <b:includable id="threadedComments"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="BlogArchive">
      <b:includable id="content"/>
      <b:includable id="flat"/>
      <b:includable id="hierarchy"/>
      <b:includable id="interval"/>
      <b:includable id="posts"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="BlogSearch">
      <b:includable id="content"/>
      <b:includable id="searchForm"/>
      <b:includable id="searchSubmit"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Header">
      <b:includable id="behindImageStyle"/>
      <b:includable id="description"/>
      <b:includable id="image"/>
      <b:includable id="title"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Label">
      <b:includable id="cloud"/>
      <b:includable id="content"/>
      <b:includable id="list"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="PageList">
      <b:includable id="content"/>
      <b:includable id="overflowButton"/>
      <b:includable id="overflowablePageList"/>
      <b:includable id="pageLink"/>
      <b:includable id="pageList"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Profile">
      <b:includable id="authorProfileImage"/>
      <b:includable id="content"/>
      <b:includable id="defaultProfileImage"/>
      <b:includable id="profileImage"/>
      <b:includable id="teamProfile"/>
      <b:includable id="teamProfileLink"/>
      <b:includable id="userGoogleProfile"/>
      <b:includable id="userLocation"/>
      <b:includable id="userProfile"/>
      <b:includable id="userProfileData"/>
      <b:includable id="userProfileImage"/>
      <b:includable id="userProfileInfo"/>
      <b:includable id="userProfileLink"/>
      <b:includable id="userProfileText"/>
      <b:includable id="viewProfileLink"/>
    </b:defaultmarkup>
    <b:defaultmarkup type="Subscribe">
      <b:includable id="content"/>
      <b:includable id="feeds"/>
    </b:defaultmarkup>
  </b:defaultmarkups>
  <b:include cond="not data:view.isPreview" data="{ selector: &quot;body&quot;, image: data:skin.vars.body_background.image }" name="responsiveImageStyle"/>
</head>
<body>
  <b:include name="canvas:bodyClass"/>
  <b:section id="header">
    <b:widget id="Header1" type="Header" version="2" locked="true">
      <b:includable id="main">
        <b:include name="behindImageStyle"/>
      </b:includable>
      <b:includable id="behindImageStyle">
        <b:include cond="data:image" name="image"/>
        <b:include cond="data:imagePlacement == &quot;BEHIND&quot;" name="title"/>
        <b:include cond="data:description and data:imagePlacement != &quot;REPLACE&quot;" name="description"/>
      </b:includable>
      <b:includable id="description"><data:description/></b:includable>
      <b:includable id="image"><a expr:href="data:blog.homepageUrl"><img expr:alt="data:title" expr:height="data:height" expr:src="data:image" expr:width="data:width"></img></a></b:includable>
      <b:includable id="title"><a expr:href="data:blog.homepageUrl"><data:title/></a></b:includable>
    </b:widget>
  </b:section>
  <b:section id="content">
    <b:widget id="Blog1" type="Blog" version="2" locked="true">
      <b:widget-settings>
        <b:widget-setting name="showCommentLink">true</b:widget-setting>
      </b:widget-settings>
      <b:includable id="main" var="this">
        <b:loop values="data:posts" var="post">
          <b:include data="post" name="bylineRegion"/>
        </b:loop>
        <b:include cond="data:view.isMultipleItems" name="postPagination"/>
      </b:includable>
      <b:includable id="post" var="post">
        <b:if cond="data:view.isMultipleItems">
          <b:comment>Feed</b:comment>
          <b:elseif cond="data:view.isPost"/>
          <b:comment>Post</b:comment>
          <b:include name="postMeta" data="post"/>
          <b:include name="comments" data="post"/>
          <b:elseif cond="data:view.isPage"/>
          <b:comment>Page</b:comment>
        </b:if>
      </b:includable>
      <b:includable id="bylineRegion" var="post">
        <b:with value="data:widgets.Blog1.allBylineItems" var="show">
          <b:include data="post" name="post"/>
        </b:with>
      </b:includable>
      <b:includable id="commentAuthorAvatar">
        <b:include name="canvas:image" data="{src: (data:comment.authorAvatarSrc contains &quot;/img/bl&quot; ?  data:skin.vars.d_avatar : data:comment.authorAvatarSrc), resize: 40, ratio: &quot;1:1&quot; }"></b:include>
      </b:includable>
      <b:includable id="commentDeleteIcon"><a expr:class="data:comment.adminClass" expr:href="data:comment.deleteUrl"><data:messages.deleteComment/></a></b:includable>
      <b:includable id="commentReplyIcon">
        <b:if cond="not data:reply and data:post.allowNewComments and data:skin.vars.c_replies"><a expr:data-parent-id="data:comment.id" href="#reply"><data:skin.vars.t_reply/></a></b:if>
      </b:includable>
      <b:includable id="commentForm">
        <iframe expr:src="data:post.commentFormIframeSrc params {skin: &quot;emporio&quot;, blogID: data:blog.blogId }" frameborder="0" width="100%" height="200" loading="lazy" id="comment-editor"></iframe>
      </b:includable>
      <b:includable id="comments" var="post">
        <b:if cond="not data:view.isPreview and data:post.allowComments">
          <b:switch var="data:skin.vars.c_comments">
            <b:case value="blogger"/>
            <b:include data="post" name="commentList"/>
            <b:default/>
            <div id="disqus_thread" expr:data-post-url="data:view.url.canonical" expr:data-post-id="data:post.id" expr:data-shortname="data:skin.vars.c_comments"></div>
          </b:switch>
        </b:if>
      </b:includable>
      <b:includable id="commentList" var="post">
        <div class="comments" id="comments">
          <div class="comments-content">
            <b:loop expr:reverse="data:skin.vars.c_newFirst" values="data:post.comments where (c =&gt; (data:skin.vars.c_replies ? not c.inReplyTo : c.id))" var="comment">
              <div class="comments-item">
                <b:include name="commentItem" data="comment"/>
                <b:if cond="data:skin.vars.c_replies">
                  <div class="comments-replies">
                    <b:loop values="data:post.comments where (c =&gt; c.inReplyTo == data:comment.id)" var="reply">
                      <b:include name="commentItem" data="reply"/>
                    </b:loop>
                    <div class="comment-replies-form" expr:id="&quot;c&quot; + data:comment.id + &quot;-ce&quot;"></div>
                  </div>
                </b:if>
              </div>
            </b:loop>
          </div>
          <b:if cond="data:post.commentPagingRequired">
            <div class="comments-paging">
              <b:if cond="data:post.hasOlderLinks"><a expr:href="data:post.oldestLinkUrl"><data:messages.oldest/></a><a expr:href="data:post.olderLinkUrl"><data:messages.older/></a></b:if><data:post.commentRangeText/>
              <b:if cond="data:post.hasNewerLinks"><a expr:href="data:post.newerLinkUrl"><data:messages.newer/></a><a expr:href="data:post.newestLinkUrl"><data:messages.newest/></a></b:if>
            </div>
          </b:if>
          <div class="comments-form" id="comment-form-thread">
            <b:include cond="data:post.allowNewComments" data="post" name="commentForm"/>
          </div>
        </div>
      </b:includable>
      <b:includable id="commentItem" var="comment">
        <div class="comment" expr:id="data:comment.cmtBodyIdPostfix">
          <div class="comment-avatar">
            <b:include name="commentAuthorAvatar"/>
          </div>
          <div class="comment-block">
            <div class="comment-header">
              <b:tag name="a" cond="data:comment.authorUrl" expr:href="data:comment.authorUrl"><data:comment.author/></b:tag><data:comment.timestamp/>
            </div>
            <div class="comment-body">
              <b:class cond="data:comment.isDeleted" name="comment-deleted"/>
              <b:eval expr="data:skin.vars.c_linkFilter and data:comment.body contains &quot;&lt;/a&gt;&quot; ? data:skin.vars.t_nolinks : data:comment.body"></b:eval>
            </div>
            <div class="comment-footer">
              <b:include name="commentReplyIcon"/>
              <b:include name="commentDeleteIcon"/>
            </div>
          </div>
        </div>
      </b:includable>
      <b:includable id="nextPageLink"><a expr:href="data:olderPageUrl"><data:messages.olderPosts/></a></b:includable>
      <b:includable id="previousPageLink"><a expr:href="data:newerPageUrl"><data:messages.newerPosts/></a></b:includable>
      <b:includable id="postPagination">
        <b:tag id="blog-pager" cond="data:newerPageUrl or data:olderPageUrl" name="div">
          <b:include cond="data:newerPageUrl" name="previousPageLink"/>
          <b:include cond="data:olderPageUrl" name="nextPageLink"/>
        </b:tag>
      </b:includable>
      <b:includable id="postMeta" var="post">
        <script type="application/ld+json">
          {
            "@context": "http://schema.org",
            "@type": "BlogPosting",
            "mainEntityOfPage": {
              "@type": "WebPage",
              "@id": "<data:post.url.canonical.jsonEscaped/>"
            },
            "headline": "<data:post.title.jsonEscaped/>",
            "description": "<b:eval expr='(data:post.snippets.long snippet { length: 150 }).jsonEscaped'/>",
            "datePublished": "<data:post.date.iso8601.jsonEscaped/>",
            "dateModified": "<data:post.lastUpdated.iso8601.jsonEscaped/>",
            <b:include name='postMetadataJSONImage'/>
            "author": {
              "@type": "Person",
              <b:if cond="data:post.author.profileUrl">
                "url": "<data:post.author.profileUrl.jsonEscaped/>",
              </b:if>
              "name": "<data:post.author.name.jsonEscaped/>"
            }
          }
        </script>
      </b:includable>
      <b:includable id="postMetadataJSONImage">
        <b:with value="resizeImage((data:post.featuredImage ?: data:skin.vars.d_image), 1200, &quot;16:9&quot;)" var="image">
          <b:if cond="data:image">
            "image": {
              "@type": "ImageObject",
              "url": "<b:eval expr='data:image.jsonEscaped'/>",
              "width": <b:eval expr='data:image.width'/>,
              "height":  <b:eval expr='data:image.height'/>
            },
          </b:if>
        </b:with>
      </b:includable>
    </b:widget>
  </b:section>
</body>
</html>